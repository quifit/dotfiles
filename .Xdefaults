*background: #002b36
*foreground: #657b83
*cursorColor: #93a1a1

!!enable perl extentions for copy/paste and clickable URLs 
URxvt.perl-ext-common: default,clipboard,matcher

!! set up copy/paste
URxvt.keysym.Shift-Control-V: perl:clipboard:paste
URxvt.iso14755: False


!! set up clickable URLs with firefox and left mouse button
URxvt.urlLauncher: /usr/bin/firefox 
URxvt.matcher.button: 1

!!Fonts
URxvt*font: 9x15bold,xft:Bitstream Vera Sans Mono
!UXTerm*font: -xos4-terminus-medium-r-normal-*-*-160-*-*-*-*-iso10646-1
!URxvt.font: xft:DejaVu Sans Mono:pixelsize=14

!! black dark/light
*color0: #073642
*color8: #002b36

!! red dark/light
*color1: #dc322f
*color9: #cb4b16

!! green dark/light
*color2: #859900
*color10: #586e75

!! yellow dark/light
*color3: #b58900
*color11: #657b83

!! blue dark/light
*color4: #268bd2
*color12: #839496

!! magenta dark/light
*color5: #d33682
*color13: #6c71c4

!! cyan dark/light
*color6: #2aa198
*color14: #93a1a1

!! white dark/light
*color7: #eee8d5
*color15: #fdf6e3

URxvt*depth: 32
URxvt*background:[93]#000000
rgba:0000/0000/0200/c800

URxvt.scrollBar: false
